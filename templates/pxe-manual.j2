default menu.c32
prompt 0
timeout 300
serial 0 115200

MENU TITLE PXE Menu

LABEL {{ distro }} {{ version }} Manual Install
        MENU LABEL {{ distro }} {{ version }} Manual Install {{ nic }}
        KERNEL stage2/{{ distro }}{{ version }}/vmlinuz
        APPEND initrd=stage2/{{ distro }}{{version}}/initrd.img ramdisk_size=100000 ksdevice={{ nic }} ip=dhcp console=ttyS1,115200n8 inst.stage2=nfs:{{ bootserver}}:/home/media/os/{{ distro }}{{ version}}/
